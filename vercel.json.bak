{
    "version": 2,
    "framework": "vuepress",
    "routes": [
        {
            "src": "/hvv-2023",
            "dest": "/"
        },
        {
            "src": "/hvv-2023/(.*)",
            "dest": "/$1"
        }
    ],
    "git": {
        "deploymentEnabled": {
            "master": true,
            "gh-pages": false
        }
    },
    "buildCommand": "yarn docs:build",
    "outputDirectory": "docs/.vuepress/dist",
    "installCommand": "yarn install"
}